{
    "name": "inani/larapoll",
    "description": "A Laravel package to create polls",
    "keywords": ["polls", "votes", "laravel", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Inani El Houssain",
            "email": "inanielhoussain@gmail.com"
        },
        {
            "name": "Jayenne Montana",
            "email": "hello@jayenne.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "Inani\\Larapoll\\": "src/"
      }
    },
    "require": {
      "illuminate/support": "^9.0|^10.0|^11.0",
    },
    "require-dev": {
      "fakerphp/faker": "^1.23",
      "phpunit/phpunit": "^11.0.1"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Inani\\Larapoll\\LarapollServiceProvider"
        ],
        "aliases": {
          "PollWriter": "Inani\\Larapoll\\PollWriterFacade"
        }
      }
    }
}
